class Solution {
    public int numJewelsInStones(String jewels, String stones) {
        
        int count = 0 ;
        for(int i = 0 ; i < stones.length(); i++)
        {
            for(int j = 0 ; j < jewels.length() ; j++)
            {
                char ans1 = stones.charAt(i);
                char ans2 = jewels.charAt(j);
                if(Character.compare(ans1,ans2) == 0)
                   count +=1;
            }
        }
        return count;
    }
}
